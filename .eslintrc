{
  "extends": ["taro/react"],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "import/no-commonjs": 0,
    "no-shadow": 0,
    "react/jsx-curly-brace-presence": 0,
    "jsx-quotes": 0,
    "no-unused-vars": "warn"
  },
  "globals": {
    "wx": true,
  },
}
